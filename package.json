{
  "name": "projects-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "ts-node src/server.ts",
    "build": "next build",
    "start": "NODE_ENV=production node dir/server.js",
    "lint": "eslint ."
  },
  "dependencies": {
    "@prisma/client": "^4.16.1",
    "dotenv": "^16.3.0",
    "express": "^4.18.2",
    "joi": "^17.9.2",
    "next": "13.4.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^20.2.5",
    "@types/react": "18.2.8",
    "@types/react-dom": "18.2.4",
    "eslint": "8.42.0",
    "eslint-config-next": "^12.0.4",
    "nodemon": "^2.0.22",
    "prisma": "^4.16.1",
    "ts-node": "^10.9.1",
    "typescript": "5.1.3"
  },
  "prisma": {
    "schema": "src/backend/prisma/schema.prisma",
    "generate": "src/generated/client",
    "migration": "src/backend/prisma/migrations",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} src/backend/prisma/seed/seeder.ts"
  }
}
